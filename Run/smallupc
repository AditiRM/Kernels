NUMTHREADS=4 
NUMITERS=10 
SEPLINE="===============================================================" 
 
UPC/Synch_p2p/p2p       -sched-policy cpu -heap 512M -n $NUMTHREADS $NUMITERS 1000 1000;          echo $SEPLINE
UPC/Stencil/stencil     -sched-policy cpu -heap 512M -n $NUMTHREADS $NUMITERS 1000;               echo $SEPLINE
UPC/Transpose/transpose -sched-policy cpu -heap 512M -n $NUMTHREADS $NUMITERS 2000 64;            echo $SEPLINE
