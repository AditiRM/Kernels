NUMPROCS=2
NUMTHREADS=2
NUMITERS=10
SEPLINE="==============================================================="
MPIRUN=mpirun

$MPIRUN -np $NUMPROCS MPIOPENMP/Stencil/stencil       $NUMTHREADS $NUMITERS 1000;       echo $SEPLINE
$MPIRUN -np $NUMPROCS MPIOPENMP/Transpose/transpose   $NUMTHREADS $NUMITERS 2000 64;    echo $SEPLINE


